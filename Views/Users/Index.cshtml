@model IEnumerable<dejtingsajt.Models.ApplicationUser>

@{
    ViewData["Title"] = "Index";
}

<h1>Datig </h1>


<ul>
    @foreach (var item in Model)
    {

    <li>
        @if (item.Photo != null)
        {
            <img style="width:40px;height:40px; object-fit:cover; border-radius:30px" src="data:image/*;base64,@(Convert.ToBase64String(item.Photo))">
        }
        else
        {
            <img id="profilePicture" style="width: 40px; height: 40px; object-fit: cover; border-radius: 30px" src="/images/download.jpg">
        }

        @Html.ActionLink(item.Name, "Details", new { id = item.Id })
    </li>


    }
</ul>
